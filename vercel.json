{
  "redirects": [
    {
      "source": "/sitemap.xml",
      "has": [
        {
          "type": "host",
          "value": "thevistassummerlin.com"
        }
      ],
      "destination": "https://www.thevistassummerlin.com/sitemap.xml",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.thevistassummerlin.com"
        }
      ],
      "destination": "https://thevistassummerlin.com/$1",
      "permanent": true
    }
  ],
  "functions": {
    "app/page.tsx": {
      "maxDuration": 30
    }
  }
}
